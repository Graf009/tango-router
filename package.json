{
  "name": "@eagle/tango-router",
  "version": "1.1.2",
  "description": "Dead simple router for @eagle/tango",
  "repository": {
    "type": "git",
    "url": "https://github.com/Graf009/tango-router.git"
  },
  "author": "Oleg Orlov <mail@orlovoleg.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run check && npm run build",
    "clean": "rimraf dist es",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs ava",
    "check": "npm run lint && npm run test",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es"
  },
  "peerDependencies": {
    "@eagle/tango": "^1.0.0"
  },
  "dependencies": {
    "babel-runtime": "6.9.2",
    "immutable": "3.8.1",
    "invariant": "2.2.1",
    "react-router": "2.6.0",
    "react-router-redux": "3.0.0"
  },
  "devDependencies": {
    "@eagle/eslint-config-eagle": "1.2.4",
    "@eagle/tango": "1.2.0",
    "ava": "0.15.2",
    "babel-cli": "6.10.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-es2015-loose-native-modules": "1.0.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "cross-env": "2.0.0",
    "eslint": "2.13.1",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.2.2",
    "eslint-plugin-react": "5.1.1",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "rimraf": "2.5.3"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
